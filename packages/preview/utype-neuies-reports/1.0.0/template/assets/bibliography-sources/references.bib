	note = {Accepted: 2021-11-12T15:31:55Z
Publisher: Akdeniz Üniversitesi},
	keywords = {hakkında/PISA/2015, hakkında/PISA/beceri/fen-bilgisi-okuryazarlığı, hakkında/PISA/beceri/okuma, YL-Tez/literatür/uzak/incelendi},
	file = {Uluğ - 2019 - PISA 2015 Türkiye uygulamasında bazı öğrenci değiş.pdf:D\:\\Users\\ertugruluyar\\Documents\\Zotero\\storage\\655EXEYF\\Uluğ - 2019 - PISA 2015 Türkiye uygulamasında bazı öğrenci değiş.pdf:application/pdf},
